            <profile name="TOO SLOW!" typeId="30ff-37ca-2012-7d18" typeName="Stratagem" hidden="false" id="b894-5c51-6b63-d79f">
              <characteristics>
                <characteristic name="Cost" typeId="c27c-0e5a-de9d-b3e0">1 CP</characteristic>
